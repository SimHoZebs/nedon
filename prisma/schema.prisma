// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  ACCESS_TOKEN String?
  PUBLIC_TOKEN String? 
  ITEM_ID String?
  TRANSFER_ID String?
  PAYMENT_ID String?
}
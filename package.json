{
  "name": "nedon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker compose up -d && next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "db-up": "docker compose up -d",
    "db-down": "docker compose down",
    "db-it": "docker exec -it nedon-db psql -U postgres -d nedon",
    "format": "prettier --write . && biome check . --apply",
    "biome": "biome check .",
    "tsc": "tsc",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "migrate-prod": "dotenv -e .env.prod prisma migrate deploy",
    "reset-prod": "dotenv -e .env.prod prisma migrate reset"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.3.2",
    "@prisma/client": "^6.1.0",
    "@supabase/supabase-js": "^2.47.10",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.62.8",
    "@trpc/client": "11.0.0-next-beta.228",
    "@trpc/next": "11.0.0-next-beta.228",
    "@trpc/react-query": "11.0.0-next-beta.228",
    "@trpc/server": "11.0.0-next-beta.228",
    "framer-motion": "^11.15.0",
    "next": "15.1.2",
    "openai": "^4.77.0",
    "papaparse": "^5.4.1",
    "plaid": "30.0.0",
    "prisma-json-types-generator": "^3.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-plaid-link": "^3.6.1",
    "recharts": "^2.15.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.24.1",
    "zod-prisma-types": "^3.2.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@iconify/json": "^2.2.287",
    "@iconify/tailwind": "^1.2.0",
    "@tanstack/react-query-devtools": "^5.62.8",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/node": "22.10.2",
    "@types/papaparse": "^5.3.15",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.17.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.2",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^2.0.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.12",
      "@types/react-dom": "18.3.1"
    }
  }
}

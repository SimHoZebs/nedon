{
  "name": "nedon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker compose up -d && next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "db-up": "docker compose up -d",
    "db-down": "docker compose down",
    "db-it": "docker exec -it nedon-db psql -U postgres -d nedon",
    "format": "prettier --write . && biome check . --apply",
    "biome": "biome check .",
    "tsc": "tsc",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "migrate-prod": "dotenv -e .env.prod prisma migrate deploy",
    "reset-prod": "dotenv -e .env.prod prisma migrate reset"
  },
  "dependencies": {
    "@google-cloud/vision": "^5.3.3",
    "@libsql/client": "^0.15.14",
    "@prisma/client": "^6.15.0",
    "@supabase/supabase-js": "^2.56.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.85.6",
    "@trpc/client": "11.5.0",
    "@trpc/next": "11.5.0",
    "@trpc/react-query": "11.5.0",
    "@trpc/server": "11.5.0",
    "framer-motion": "^12.23.12",
    "next": "15.5.2",
    "openai": "^5.16.0",
    "papaparse": "^5.5.3",
    "plaid": "38.0.0",
    "prisma-json-types-generator": "^3.6.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-plaid-link": "^4.1.1",
    "recharts": "^3.1.2",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5",
    "zod-prisma-types": "^3.2.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@iconify/json": "^2.2.380",
    "@iconify/tailwind": "^1.2.0",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "24.3.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "autoprefixer": "^10.4.21",
    "cypress": "^15.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.15.0",
    "tailwindcss": "^4.1.12",
    "typescript": "5.9.2",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.12",
      "@types/react-dom": "18.3.1"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@prisma/client",
      "@prisma/engines",
      "cypress",
      "esbuild",
      "prisma",
      "protobufjs",
      "sharp"
    ]
  }
}
